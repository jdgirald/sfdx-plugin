{
    "version": "0.3.0",
    "commands": {
        "muenzpraeger:source:cleanupaura": {
            "id": "muenzpraeger:source:cleanupaura",
            "description": "Removes all boilerplate Aura related files that have not been modified",
            "pluginName": "@muenzpraeger/sfdx-plugin",
            "pluginType": "core",
            "aliases": [],
            "examples": [
                "$ sfdx muenzpraeger:source:cleanupaura\n     Make sure that your git commits are up-to-date before you proceed. Do you want to delete boilerplate Aura related files that have not been modified? (y/N) y\n     Reading content of package directories\n     36 non-modified boilerplate Aura files have been deleted in package directory 'force-app'.\n  ",
                "$ sfdx muenzpraeger:source:cleanupaura -p\n     Reading content of package directories\n     36 non-modified boilerplate Aura files have been deleted in package directory 'force-app'.\n"
            ],
            "flags": {
                "json": {
                    "name": "json",
                    "type": "boolean",
                    "description": "format output as json",
                    "required": false
                },
                "loglevel": {
                    "name": "loglevel",
                    "type": "option",
                    "description": "logging level for this command invocation",
                    "required": false,
                    "helpValue": "(trace|debug|info|warn|error|fatal)",
                    "options": [
                        "trace",
                        "debug",
                        "info",
                        "warn",
                        "error",
                        "fatal"
                    ]
                },
                "help": {
                    "name": "help",
                    "type": "boolean",
                    "char": "h",
                    "description": "show CLI help"
                },
                "noprompt": {
                    "name": "noprompt",
                    "type": "boolean",
                    "char": "p",
                    "description": "no prompt for confirm deletion"
                }
            },
            "args": []
        },
        "muenzpraeger:swagger:import": {
            "id": "muenzpraeger:swagger:import",
            "description": "Auto-generate Apex classes from Swagger/OpenAPI files.",
            "pluginName": "@muenzpraeger/sfdx-plugin",
            "pluginType": "core",
            "aliases": [],
            "examples": [
                "$ sfdx muenzpraeger:swagger:import -d . -p http://petstore.swagger.io/v2/swagger.json\n  Apex classes have been generated.\n  "
            ],
            "flags": {
                "json": {
                    "name": "json",
                    "type": "boolean",
                    "description": "format output as json",
                    "required": false
                },
                "loglevel": {
                    "name": "loglevel",
                    "type": "option",
                    "description": "logging level for this command invocation",
                    "required": false,
                    "helpValue": "(trace|debug|info|warn|error|fatal)",
                    "options": [
                        "trace",
                        "debug",
                        "info",
                        "warn",
                        "error",
                        "fatal"
                    ]
                },
                "targetdevhubusername": {
                    "name": "targetdevhubusername",
                    "type": "option",
                    "char": "v",
                    "description": "username or alias for the dev hub org; overrides default dev hub org",
                    "required": false
                },
                "apiversion": {
                    "name": "apiversion",
                    "type": "option",
                    "char": "a",
                    "description": "specify the API version (defaults to API version of your DevHub)",
                    "required": false
                },
                "help": {
                    "name": "help",
                    "type": "boolean",
                    "char": "h",
                    "description": "show CLI help"
                },
                "path": {
                    "name": "path",
                    "type": "option",
                    "char": "p",
                    "description": "URL or local file path for Swagger definition file.",
                    "required": true
                },
                "outputdir": {
                    "name": "outputdir",
                    "type": "option",
                    "char": "d",
                    "description": "local folder for storing the created files.",
                    "required": true
                },
                "classprefix": {
                    "name": "classprefix",
                    "type": "option",
                    "char": "c",
                    "description": "specify a class prefix (defaults to \"Swag\")"
                },
                "force": {
                    "name": "force",
                    "type": "boolean",
                    "char": "f",
                    "description": "overwrites existing files"
                }
            },
            "args": []
        },
        "muenzpraeger:source:api:set": {
